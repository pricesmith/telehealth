  },
  "scripts": {
    "postinstall": "yarn build:tailwind",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "build:tailwind": "tailwind build app/javascript/src/tailwind.css -o app/javascript/src/tailwind.output.css",
    "watch:tailwind": "chokidar 'app/javascript/src/**/*.css' 'app/javascript/src/**/*.scss' --ignore app/javascript/src/tailwind.output.css -c 'npm run build:tailwind'",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "start:react": "react-scripts start",
    "prebuild": "run-s build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
